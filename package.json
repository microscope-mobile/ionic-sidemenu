{
  "name": "ionic-sidemenu",
  "version": "1.1.0",
  "description": "microscope mobile starter kit with ionic sidemenu template",
  "scripts": {
    "start": "http-server ./www -s",
    "serve": "gulp serve",
    "test": "protractor ./tests/config.js",
    "lint": "gulp lint",
    "build": "gulp build",
    "build:android": "gulp build && ionic build android",
    "build:ios": "gulp build && ionic build ios",
    "release": "gulp release",
    "release:android": "gulp release && ionic build android --release",
    "release:ios": "gulp release && ionic build ios --release",
    "ios": "gulp build && ionic run ios",
    "android": "gulp build && ionic run android",
    "emulate:ios": "gulp build && ionic emulate ios",
    "emulate:android": "gulp build && ionic emulate android"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-animate": "^1.5.7",
    "angular-sanitize": "^1.5.7",
    "angular-ui-router": "^0.3.1",
    "http-server": "^0.9.0",
    "ionic-npm": "^1.0.2",
    "lodash": "^3.9.3",
    "ng-cordova": "^0.1.26-alpha"
  },
  "devDependencies": {
    "browserify": "^10.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "gulp": "^3.8.11",
    "gulp-browserify": "^0.5.1",
    "gulp-concat": "^2.2.0",
    "gulp-connect": "^2.2.0",
    "gulp-jshint": "^1.9.2",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^2.2.14",
    "jshint-stylish": "^1.0.0",
    "run-sequence": "^1.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ]
}
